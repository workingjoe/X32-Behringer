{
    "configurations": [
        {
            "name": "windows_make",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/X32lib"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "windowsSdkVersion": "10.0.22621.0",
            "compilerPath": "C:/msys64/mingw64/bin/gcc.exe",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "intelliSenseMode": "${default}",
            "compilerArgs": [
                "-O0",
                "-g3",
                "-Wall",
                "-o build",
                "-LX32Lib",
                "-lm",
                "-lX32",
                "-lws2_32",
                "-lgdi32",
                "-lcomdlg32",
                "-lwinmm",
                "-luser32"
            ]           
        }
    ],
    "version": 4
}